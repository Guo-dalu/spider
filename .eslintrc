{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "airbnb-base"
  ],
  "plugins": [
    "babel",
    "import"
  ],
  "settings": {
    "import/ignore": [
      "node_modules",
      "bin/*",
      "app.js",
      "\\.json$"
    ],
    "import/parser": "babel-eslint",
    "import/resolve": {
      "extensions": [
        ".js",
        ".json"
      ]
    }
  },
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "import/extensions": "never",
    "no-nested-ternary": 0,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-tabs": 0,
    "class-methods-use-this": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-undef": 1,
    "id-length": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0,
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-await-in-loop": 0,
    "max-len": [
      2,
      120,
      2,
      {
        "ignoreUrls": true,
        "ignoreComments": false
      }
    ],
    "new-cap": [
      2,
      {
        "capIsNewExceptions": [
          "express.Router",
          "Radium"
        ]
      }
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  },
  "globals": {
    "logger": true,
    "debug": true,
    "outputLog": true,
    "__SERVER__": true,
    "__CLIENT__": true,
    "__ENV__": true,
    "__DEVTOOLS__": true,
    "__LOGGER__": true,
    "__DEVELOPMENT__": true,
    "__IS_PRODUCTION__": true,
    "__PRODUCTION__": true,
    "__TEST__": true,
    "__IS_MESSAGE_OPEN__": true,
    "__ALLOWED_EXPORTDATA__": true,
    "FileReader": true,
    "navigator": true,
    "sa": true,
    "describe": true,
    "it": true,
    "Promise": true,
    "localStorage": true,
    "console": true,
    "XMLHttpRequest": true,
    "System": true,
    "FormData": true,
    "window": true,
    "process": true,
    "sessionStorage": true,
    "location": true
  }
}